{
  "extends": "ts-node/node16/tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "lib": [
      "es2015"
    ],
    "module": "commonjs",
    "declaration": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "allowJs": false,
    "newLine": "LF"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
